page {
    background-color: #f4f4f4;
}

.emptyPage {
    width: 100%;
    height: 100%;
    text-align: center;
    align-content: center;
}

.emptyPage image {
    margin-top: 240rpx;
    width: 252rpx;
    height: 252rpx;
}

.emptyPage view {
    margin-top: 120rpx;
    color: #b2b2b2;
    font-size: 32rpx;
}

.emptyPage button {
    margin-top: 24rpx;
    color: #272727;
    font-size: 26rpx;
    height: 54rpx;
    line-height: 50rpx;
    border-radius: 27rpx;
    width: 154rpx;
    border: 1rpx solid #e6e6e6;
}

.emptyPage button::after {
    border: none;
}

.listView {
    margin: 0 40rpx 40rpx;
}

.cellView {
    position: relative;
    margin-top: 32rpx;
    height: 252rpx;
    overflow: hidden;
    border-radius: 10rpx;
}

.cellInfo {
    position: absolute;
    top: 0;
    background-color: white;
    width: 670rpx;
    height: 252rpx;
    z-index: 5;
    transition: left 0.2s ease-in-out;
    overflow: hidden;
    display: flex;
    flex-direction: row;
    justify-content: space-start;
}

.commodityPic {
    margin: 32rpx 0 32rpx 32rpx;
    height: 188rpx;
    width: 188rpx;
    border-radius: 8rpx;
}

.rightView {
    margin: 32rpx 32rpx 0 16rpx;
    height: 188rpx;
    position: relative;
    flex: 1;
}

.nameInfo {
    color: #272727;
    font-size: 20rpx;
}

.priceInfo {
    font-size: 22rpx;
    position: absolute;
    left: 0;
    bottom: 0;
}

.cellDelete {
    position: absolute;
    top: 0;
    right: 3rpx;
    background-color: #E84B57;
    width: 206rpx;
    height: 252rpx;
    text-align: center;
    line-height: 252rpx;
    font-size: 32rpx;
    z-index: 4;
    color: white;
}

.haowuquanImage {
    margin-top: 64rpx;
    height: 47rpx;
    width: 184rpx;
    margin-left: 243rpx;
}