page {
    background-color: #f4f4f4;
}

.pageInfo {
    padding-bottom: constant(safe-area-inset-bottom);
    padding-bottom: env(safe-area-inset-bottom);
}

.whiteView {
    margin: 32rpx 40rpx 0;
    border-radius: 10rpx;
    background-color: white;
    padding: 0 8rpx;
    height: 140rpx;
}

.dataInfoCell {
    margin-top: 32rpx;
    height: 76rpx;
    display: inline-block;
    border-right: 1rpx solid #e6e6e6;
    width: 217rpx;
}

.dataInfoTop {
    height: 48rpx;
    line-height: 48rpx;
    font-weight: bold;
    color: #363737;
    font-size: 40rpx;
    text-align: center;
    overflow: hidden;
}

.dataInfoBottom {
    margin-top: 8rpx;
    height: 20rpx;
    line-height: 20rpx;
    color: #B2B2B2;
    font-size: 20rpx;
    text-align: center;
    overflow: hidden;
}

.orderCell {
    background-color: white;
    margin: 32rpx 40rpx;
    border-radius: 15rpx;
    padding: 0 32rpx;
}

.orderCellTop {
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
    justify-content: space-between;
    padding: 24rpx 0 32rpx;
    font-size: 24rpx;
}

.orderCellTopLeft {
    display: flex;
    flex-wrap: nowrap;
    align-items: flex-start;
    justify-content: space-between;
    color: #272727;
    font-weight: bold;
}

.orderCellTopLeft image {
    width: 32rpx;
    height: 32rpx;
    margin-right: 8rpx;
}

.orderCellTopData {
    margin-left: 8rpx;
    color: #B2B2B2;
    font-weight: normal;
}

.orderStatus {
    display: flex;
    flex-wrap: nowrap;
    align-items: flex-end;
    justify-content: space-between;
    color: #B2B2B2;
}

.orderInfo {
    height: 132rpx;
    border-bottom: 1rpx solid #e6e6e6;
    padding-bottom: 32rpx;
}

.orderInfoOne {
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
    justify-content: space-between;
}

.orderInfoOneImage {
    border-radius: 15rpx;
    flex-grow: 0;
    flex-shrink: 0;
    height: 132rpx;
    width: 132rpx;
    margin-right: 16rpx;
    border: 1rpx solid #e6e6e6;
}

.orderInfoOneRight {
    flex: 1;
    color: #272727;
    font-size: 20rpx;
    align-content: center;
}

.pdSkuTypeValsOnlyValStr {
    margin-top: 12rpx;
    font-weight: bold;
    font-size: 20rpx;
    color: #272727;
}

.orderInfoMoreImage {
    border-radius: 15rpx;
    flex-grow: 0;
    flex-shrink: 0;
    height: 132rpx;
    width: 132rpx;
    margin-right: 22rpx;
    border: 1rpx solid #e6e6e6;
    position: relative;
}

.orderInfoMoreImageQty {
    position: absolute;
    right: 0;
    top: 0;
    height: 24rpx;
    min-width: 24rpx;
    font-size: 20rpx;
    line-height: 20rpx;
    padding: 8rpx;
    background-color: rgba(54,55,55,0.3);
    color: white;
}

.orderInfoMoreLookMore {
    border-radius: 15rpx;
    background-color: #f4f4f4;
    color: #363737;
    font-size: 22rpx;
    height: 132rpx;
    width: 132rpx;
    text-align: center;
    line-height: 132rpx;
    display: inline-block;
    vertical-align: top;
}

.orderCellBottom {
    height: 82rpx;
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
    justify-content: space-between;
}

.orderCellBottomLeft {
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
    justify-content: flex-start;
    font-size: 22rpx;
    color: #B2B2B2;
}

.orderCellBottomLeft image {
    height: 34rpx;
    width: 34rpx;
    border-radius: 18rpx;
    border: 1rpx solid #e6e6e6;
    margin-right: 8rpx;
}

.orderCellBottomRight {
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
    justify-content: flex-end;
    font-size: 24rpx;
    color: #272727;
}

.orderCellBottomRightSymbol {
    margin-left: 16rpx;
    margin-right: 16rpx;
    color: #e6e6e6;
}

.payTypeCard {
    margin-left: 8rpx;
    background-color: #F3B356;
    border-radius: 0 6rpx 6rpx 6rpx;
    color: white;
    height: 30rpx;
    width: 68rpx;
    line-height: 30rpx;
    text-align: center;
    font-size: 18rpx;
}

.payTypeWechat {
    margin-left: 8rpx;
    background-color: #30BE76;
    border-radius: 0 6rpx 6rpx 6rpx;
    color: white;
    height: 30rpx;
    width: 85rpx;
    line-height: 30rpx;
    text-align: center;
    font-size: 18rpx;
}

.footerView {
    text-align: center;
    color: #B2B2B2;
    font-size: 22rpx;
    margin: 32rpx 40rpx;
}