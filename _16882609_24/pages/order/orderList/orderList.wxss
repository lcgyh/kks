    font-size: 24rpx;
}

.orderCellTopLeft {
    display: flex;
    flex-wrap: nowrap;
    align-items: flex-start;
    justify-content: space-between;
    color: #272727;
    font-weight: bold;
}

.orderCellTopLeft image {
    width: 32rpx;
    height: 32rpx;
    margin-right: 8rpx;
}

.orderInfo {
    height: 132rpx;
    border-bottom: 1rpx solid #e6e6e6;
    padding-bottom: 32rpx;
}

.orderInfoOne {
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
    justify-content: space-between;
}

.orderInfoOneImage {
    border-radius: 15rpx;
    flex-grow: 0;
    flex-shrink: 0;
    height: 132rpx;
    width: 132rpx;
    margin-right: 16rpx;
    border: 1rpx solid #e6e6e6;
}

.orderInfoOneRight {
    flex: 1;
    color: #272727;
    font-size: 20rpx;
