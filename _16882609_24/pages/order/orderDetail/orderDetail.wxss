page {
    background-color: #f4f4f4;
}

.pageInfo {
    padding-bottom: constant(safe-area-inset-bottom);
    padding-bottom: env(safe-area-inset-bottom);
    position: relative;
}

.navBar {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    z-index: 99;
    background-color: white;
}

.navBarTitle {
    position: relative;
    width: 100%;
    height: 44px;
    line-height: 44px;
    text-align: center;
    color: #272727;
    font-size: 38rpx;
    font-weight: bold;
}

.navBarBackButtonImage {
    position: absolute;
    width: 18px;
    height: 18px;
    top: 13px;
    left: 40rpx;
}

.backButton {
    position: absolute;
    width: 44px;
    height: 44px;
    top: 0;
    left: 32rpx;
}

.headerImage {
    position: relative;
    width: 750rpx;
    height: 360rpx;
}

.orderStatus {
    position: absolute;
    color: white;
    font-size: 48rpx;
    line-height: 48rpx;
    left: 40rpx;
    bottom: 76rpx;
    font-weight: bold;
}

.orderPayTime {
    position: absolute;
    color: white;
    font-size: 24rpx;
    line-height: 24rpx;
    left: 40rpx;
    bottom: 40rpx;
}

.whiteView {
    margin: 32rpx 40rpx 0;
    border-radius: 15rpx;
    background-color: white;
    padding: 0 32rpx;
}

.betweenView {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.endView {
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    align-items: center;
}

.topView {
    height: 96rpx;
    font-size: 26rpx;
    color: #363737;
    font-weight: bold;
    border-bottom: 1rpx solid #e6e6e6;
    line-height: 96rpx;
}

.topView {
    height: 96rpx;
    font-size: 26rpx;
    color: #363737;
    font-weight: bold;
    border-bottom: 1rpx solid #e6e6e6;
    line-height: 96rpx;
}

.topViewRight {
    font-weight: normal;
    font-size: 22rpx;
}

.deliveryTypeExplain {
    font-size: 22rpx;
    color: #6b6b6b;
    height: 72rpx;
    border-bottom: 1rpx solid #e6e6e6;
    line-height: 72rpx;
}

.addressView {
    padding: 32rpx 0;
    font-size: 20rpx;
    color: #272727;
}

.addressTitle {
    font-size: 26rpx;
    line-height: 26rpx;
    height: 26rpx;
    flex: 1;
    overflow: hidden;
    font-weight: bold;
    margin-bottom: 16rpx;
}

.phoneCallButton {
    color: #5f7fdc;
    height: 42rpx;
    width: 125rpx;
    line-height: 42rpx;
    border-radius: 21rpx;
    border: 1rpx solid #e6e6e6;
    font-size: 16rpx;
    background-color: white;
    margin-left: 16rpx;
}

.phoneCallButton::after {
    border: none;
}

.logisticsCell {
    border-bottom: 1rpx solid #e6e6e6;
}

.logisticsArrow {
    width: 20rpx;
    height: 20rpx;
}

.logisticsDetail {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.ocOrderDetailsQty {
    font-size: 22rpx;
    font-weight: normal;
    color: #363737;
}

.ocOrderDetailsTable {
    padding-bottom: 32rpx;
}

.ocOrderDetailsCell {
    margin-top: 32rpx;
    height: 188rpx;
}

.ocOrderDetailsCell image {
    border-radius: 15rpx;
    flex-grow: 0;
    flex-shrink: 0;
    height: 186rpx;
    width: 186rpx;
    margin-right: 16rpx;
    border: 1rpx solid #e6e6e6;
}

.ocOrderDetailsCellRigth {
    position: relative;
    flex: 1;
    height: 188rpx;
}

.itemTitle {
    color: #363737;
    font-size: 24rpx;
    display: -webkit-box;
    word-break: break-all;
    text-overflow: ellipsis;
    overflow: hidden;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.spuTagView {
    margin-top: 8rpx;
    height: 34rpx;
    overflow: hidden;
}

.spuTagView view {
    display: inline-block;
    font-size: 18rpx;
    padding: 0 12rpx;
    height: 34rpx;
    line-height: 36rpx;
    border-radius: 0px 5.6rpx 5.6rpx 5.6rpx;
    color: #5f7fdc;
    background-color: rgba(95,127,220,0.1);
    vertical-align: top;
    margin-right: 8rpx;
}

.skunameShort {
    margin-top: 8rpx;
    font-weight: bold;
    font-size: 20rpx;
    color: #363737;
}

.priceAndQty {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    height: 24rpx;
    font-size: 18rpx;
}

.priceAndQtyPrice {
    font-size: 24rpx;
    line-height: 24rpx;
    color: #5f7fdc;
    font-weight: bold;
}

.qtySymbol {
    font-size: 18rpx;
    margin-right: 4rpx;
}

.payViewInfo {
    padding: 24rpx 0;
    font-size: 22rpx;
    color: #b2b2b2;
    line-height: 30rpx;
    border-bottom: 1rpx solid #e6e6e6;
}

.payViewInfoRight {
    color: #272727;
}

.payViewInfoFreight {
    margin-top: 24rpx;
}

.payTotalView {
    height: 96rpx;
    font-size: 22rpx;
    color: #5f7fdc;
}

.payTotalViewTitle {
    color: #363737;
}

.payTypeView {
    font-size: 22rpx;
    height: 80rpx;
    color: #363737;
}

.payTypeTitle {
    color: #b2b2b2;
}

.contactButton {
    height: 56rpx;
    width: 146rpx;
    border: 1rpx solid #e6e6e6;
    border-radius: 28rpx;
    color: #272727;
    font-size: 24rpx;
    padding: 0;
    margin: 0;
    font-weight: normal;
    background-color: white;
    margin-left: 16rpx;
    line-height: 54rpx;
}

.contactButton::after {
    border: none;
}

.orderInfoDisplay {
    padding: 12rpx 0;
}

.orderInfoDisplayList {
    font-size: 22rpx;
    color: #b2b2b2;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    height: 46rpx;
}

.orderInfoDisplayRight {
    color: #272727;
}

.orderNoDisplay {
    font-weight: bold;
    display: inline-block;
}

.orderNoCopy {
    color: #5f7fdc;
    margin-left: 12rpx;
    display: inline-block;
}

.footerView {
    position: fixed;
    background-color: white;
    left: 0;
    right: 0;
    bottom: 0;
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    padding-right: 40rpx;
    z-index: 5;
    height: 112rpx;
    padding-bottom: constant(safe-area-inset-bottom);
    padding-bottom: env(safe-area-inset-bottom);
}

.footerView button {
    width: 180rpx;
    height: 60rpx;
    margin: 26rpx 20rpx 0;
    color: #272727;
    font-size: 24rpx;
    border-radius: 30rpx;
    border: 1rpx solid #e6e6e6;
    background-color: transparent;
}

.footerView button::after {
    border: none;
}

.footerView .darkButton {
    border: none;
    background-color: #5f7fdc;
    color: white;
    font-weight: bold;
}